
{
	"inbounds": [{
		"port": 1080,
		"listen": "0.0.0.0",
		"protocol": "socks",
		"domainOverride": ["tls","http"],
		"settings": {
			"auth": "noauth"  //socks的认证设置，noauth 代表不认证，由于 socks 通常在客户端使用，所以这里不认证
		}
	}],
	"outbounds": [{
			"protocol": "vmess",
			"tag":"mydoor",
			"settings": {
				"vnext": [{
					"address": "nicegeek.tk",
					"port": 443,
					"users": [{
						"id": "6acb3c59-a02b-4c79-98d9-0cf3785822bb",// ***唯一需要修改的地方*****
						"alterId": 32
					}]
				}]
			},
			"streamSettings": {
				"network": "ws",
				"security": "tls",
				"wsSettings": {
					"path": "/my_diary",
					"headers": {
						"Host": "nicegeek.tk"
					}
				}
			}
		},//第一个通道：去往VPS
		{
			"protocol": "freedom",
			"tag": "direct",
			"settings": {}
		},//第二个通道：直连
		{
      "protocol": "blackhole",
			"tag":"blocked",
      "settings": {}
    }//第三个通道：阻塞
	],
	"routing": {
		"domainStrategy": "IPOnDemand",
		"rules": [
			 {
		    "domain": [
			  "domain:qq.com"//这里添加可以校园网屏蔽的国内网址
		    ],
		    "type": "field",
		    "outboundTag": "mydoor"
		   },

			 {
        "domain": [
          "tanx.com",
          "googeadsserving.cn"//这里可以添加需要阻塞的请求，可以用来屏蔽广告
        ],
        "type": "field",
        "outboundTag": "blocked"
       },

			 {
			 "domain": [
				 "domain:amazon.com",
				 "domain:microsoft.com",
				 "domain:jd.com",
				 "domain:youku.com",
				 "domain:taobao.com",
				 "domain:baidu.com",
				 "domain:vultr.com"//这里可以添加自己需要不经过vps直连的网址，防止程序的数据库里没有
			 ],
			 "type": "field",
			 "outboundTag": "direct"
		  },

			{
        "type": "field",
        "outboundTag": "direct",
        "domain": ["geosite:cn"] // 中国大陆主流网站的域名,不经vps,直连
      },

			{
			"type": "field",
			"ip": ["geoip:private","geoip:cn"],// 中国大陆主流网站的IP,私有地址的IP,不经vps,直连
			"outboundTag": "direct"
		  }

	]
	}
}
